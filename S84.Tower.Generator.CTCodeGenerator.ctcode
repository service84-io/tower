        myself.ctcode_file.WriteLine(Concat(Concat(Concat(Concat(Concat(Concat("    function ", string_parser_class_name), " "), myself.GenerateGetterName(string_parser_field_name)), "() { return myself."), string_parser_field_name), ";}"));
