        myself.ctcode_file.WriteLine("class ParserNetwork");
        myself.ctcode_file.WriteLine("{");

        int rule_index = 0;

        while (rule_index < Size(rules)) {
            S84.Tower.dbnf.ctcode.Rule rule = Element(rules, rule_index);

            if (rule.GetName()) {
                string class_name = Concat(myself.GenerateClassName(rule.GetName()), "Parser");
                string field_name = Concat(myself.CamelCaseToSnakeCase(class_name), "_field");
                myself.ctcode_file.WriteLine(Concat(Concat(Concat(Concat("    ", class_name), " "), field_name), ";"));
                myself.ctcode_file.WriteLine(Concat(Concat(Concat(Concat(Concat(Concat("    function ", class_name), " "), myself.GenerateGetterName(field_name)), "() { return myself."), field_name), "; }"));
            }

            rule_index = rule_index + 1;
        }

        string string_parser_class_name = "StringParser";
        string string_parser_field_name = Concat(myself.CamelCaseToSnakeCase(string_parser_class_name), "_field");
        string character_parser_class_name = "CharacterParser";
        string character_parser_field_name = Concat(myself.CamelCaseToSnakeCase(character_parser_class_name), "_field");
        string character_range_parser_class_name = "CharacterRangeParser";
        string character_range_parser_field_name = Concat(myself.CamelCaseToSnakeCase(character_range_parser_class_name), "_field");

