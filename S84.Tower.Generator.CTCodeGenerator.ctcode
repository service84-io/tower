        myself.ctcode_file.WriteLine("class LengthString");
        myself.ctcode_file.WriteLine("{");
        myself.ctcode_file.WriteLine("    function void SetData(LargeString new_data) { myself.data = new_data; }");
        myself.ctcode_file.WriteLine("    function LargeString GetData() { return myself.data; }");
        myself.ctcode_file.WriteLine("    function void SetStart(int new_start) { myself.start = new_start; }");
        myself.ctcode_file.WriteLine("    function int GetStart() { return myself.start; }");
        myself.ctcode_file.WriteLine("    function void SetLength(int new_length) { myself.length = new_length; }");
        myself.ctcode_file.WriteLine("    function int GetLength() { return myself.length; }");
        myself.ctcode_file.WriteLine("    function string GetString()");
        myself.ctcode_file.WriteLine("    {");
        myself.ctcode_file.WriteLine("        string deep_data = myself.data.GetData();");
        myself.ctcode_file.WriteLine("        string result;");
        myself.ctcode_file.WriteLine("        int index = myself.start;");
        myself.ctcode_file.WriteLine("        int end = myself.start + myself.length;");
        myself.ctcode_file.WriteLine("");
