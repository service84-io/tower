class CTCodeGenerator implements S84.Tower.Generator.ctcode.Generator
{
    S84.Tower.System.ctcode.OutputStream logger;
    S84.Tower.System.ctcode.OutputStream ctcode_file;

    function int GenerateParser(S84.Tower.System.ctcode.System system, S84.Tower.dbnf.ctcode.Grammar grammar, string base_name) {
        myself.logger = system.GetLoggerDestination();
        myself.ctcode_file = system.OpenFileWriter(Concat(base_name, ".ctcode"));
        myself.logger.WriteLine("Generating CTCode file...");
        myself.WriteClasses(grammar, base_name);
        myself.logger.WriteLine("Done!");
        return 0;
    }

    function void WriteClasses(S84.Tower.dbnf.ctcode.Grammar grammar, string base_name) {
        S84.Tower.dbnf.ctcode.Rule[] rules = grammar.GetRules();
        myself.ctcode_file.WriteLine("class StringParser");
        myself.ctcode_file.WriteLine("{");
        myself.ctcode_file.WriteLine("    function bool ParseSingleSave(LengthString index, string value, StringResult result)");
        myself.ctcode_file.WriteLine("    {");
        myself.ctcode_file.WriteLine("        int index_start = index.GetStart();");
        myself.ctcode_file.WriteLine("        int index_length = index.GetLength();");
        myself.ctcode_file.WriteLine("        LargeString index_data = index.GetData();");
        myself.ctcode_file.WriteLine("        LengthString consumed_string = new LengthString;");
        myself.ctcode_file.WriteLine("        consumed_string.SetData(index.GetData());");
        myself.ctcode_file.WriteLine("        consumed_string.SetStart(index.GetStart());");
        myself.ctcode_file.WriteLine("        consumed_string.SetLength(0);");
        myself.ctcode_file.WriteLine("        String instance = new String;");
        myself.ctcode_file.WriteLine("        int value_length = Length(value);");
        myself.ctcode_file.WriteLine("");
        myself.ctcode_file.WriteLine("        if (value_length > index.GetLength())");
        myself.ctcode_file.WriteLine("        {");
        myself.ctcode_file.WriteLine("            result.SetResult(false);");
        myself.ctcode_file.WriteLine("            return false;");
        myself.ctcode_file.WriteLine("        }");
        myself.ctcode_file.WriteLine("");
        myself.ctcode_file.WriteLine("        int offset_index = 0;");
        myself.ctcode_file.WriteLine("");
        myself.ctcode_file.WriteLine("        while (offset_index < value_length)");
        myself.ctcode_file.WriteLine("        {");
        myself.ctcode_file.WriteLine("            if (index_data.GetAt(index.GetStart() + offset_index) != At(value, offset_index))");
        myself.ctcode_file.WriteLine("            {");
        myself.ctcode_file.WriteLine("                result.SetResult(false);");
        myself.ctcode_file.WriteLine("                return false;");
        myself.ctcode_file.WriteLine("            }");
        myself.ctcode_file.WriteLine("");
        myself.ctcode_file.WriteLine("            offset_index = offset_index + 1;");
        myself.ctcode_file.WriteLine("        }");
        myself.ctcode_file.WriteLine("");
        myself.ctcode_file.WriteLine("        index.SetStart(index.GetStart() + value_length);");
        myself.ctcode_file.WriteLine("        index.SetLength(index.GetLength() - value_length);");
        myself.ctcode_file.WriteLine("        consumed_string.SetLength(index.GetStart() - index_start);");
        myself.ctcode_file.WriteLine("        instance.SetLengthString(consumed_string);");
        myself.ctcode_file.WriteLine("        result.SetValue(instance);");
        myself.ctcode_file.WriteLine("        result.SetResult(true);");
        myself.ctcode_file.WriteLine("        return true;");
        myself.ctcode_file.WriteLine("    }");
        myself.ctcode_file.WriteLine("    function bool ParseSingle(LengthString index, string value)");
        myself.ctcode_file.WriteLine("    {");
        myself.ctcode_file.WriteLine("        StringResult result = new StringResult;");
        myself.ctcode_file.WriteLine("        return myself.ParseSingleSave(index, value, result);");
        myself.ctcode_file.WriteLine("    }");
        myself.ctcode_file.WriteLine("}");
