        myself.ctcode_file.WriteLine(Concat(Concat(Concat(Concat("        myself.", character_parser_field_name), " = new "), character_parser_class_name), ";"));
        myself.ctcode_file.WriteLine(Concat(Concat(Concat(Concat("        myself.", character_range_parser_field_name), " = new "), character_range_parser_class_name), ";"));

